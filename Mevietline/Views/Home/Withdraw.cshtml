@{
    ViewData["Title"] = "Hỗ trợ";
}

<div class="container w-100 mt-3">
    <form>
        <div class="row">
            <div class="col-md-6 mb-3">
                <label for="withdraw-amount" class="form-label">Số tiền rút</label>
                <input type="text" class="form-control" id="withdraw-amount" aria-describedby="withdraw-amount-help">
                <div id="withdraw-amount-help" class="form-text">Số tiền rút không được quá số dư tài khoản.</div>
            </div>
            <div class="col-md-6 mb-3">
                <label for="account-owner" class="form-label">Tên chủ tài khoản</label>
                <input type="text" class="form-control" id="account-owner">
            </div>
            <div class="col-md-6 mb-3">
                <label for="account-number" class="form-label">Số tài khoản</label>
                <input type="text" class="form-control" id="account-number">
            </div>
            <div class="col-md-6 mb-3">
                <label for="bank-name" class="form-label">Ngân hàng</label>
                <input type="text" class="form-control" id="bank-name">
            </div>
            <div class="col-md-12">
                <button type="submit" class="btn btn-primary">Rút tiền</button>
            </div>
        </div>
    </form>
</div>


<script src="~/js/bangmain_vuejs/bangmain_crud.js"></script>